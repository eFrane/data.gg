<% content_for :breadcrumb do %>
  <ol class="breadcrumb">
    <li><%= link_to "Home", root_path %></li>
    <li><%= link_to "Developers", developers_path %></li>
    <li class="active">Housing</li>
  </ol>
<% end %>

<div class="page-header">
  <h1>Housing</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-hover vertical-align-middle" width="100%">
    <thead>
      <tr>
        <th>API</th>
        <th>Version</th>
        <th>Description</th>
        <th>Updated</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
     <%= render partial: 'developers/developers_list_item',
      locals: { name: 'Local Prices', target: method(:api_v10_housing_local_prices_path), desc: 'House prices by quarter since 1981 includes numerous stats',
      updated: '12/11/2015' } %>

     <%= render partial: 'developers/developers_list_item',
      locals: { name: 'Open Prices', target: method(:api_v10_housing_open_prices_path), desc: 'House prices by quarter since 1981 includes numerous stats',
      updated: '12/11/2015' } %>

     <%= render partial: 'developers/developers_list_item',
      locals: { name: 'Bedrooms', target: method(:api_v10_housing_bedrooms_path), desc: 'Total bedrooms per unit with market since 2011',
      updated: '15/05/2015' } %>

     <%= render partial: 'developers/developers_list_item',
      locals: { name: 'Types', target: method(:api_v10_housing_types_path), desc: 'Total units with market and type for 2014',
      updated: '15/05/2015' } %>

     <%= render partial: 'developers/developers_list_item',
      locals: { name: 'Units', target: method(:api_v10_housing_units_path), desc: 'Total units by market since 2010',
      updated: '15/05/2015' } %>
    </tbody>
  </table>
</div>

<p>Source: <a href="http://gov.gg/article/112353/Quarterly-Residential-Property-Prices-and-Annual-Housing-Stock-Bulletins-2014">gov.gg</a> and <a href="http://gov.gg/property">gov.gg</a></p>