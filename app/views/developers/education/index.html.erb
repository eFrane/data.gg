<% content_for :breadcrumb do %>
<ol class="breadcrumb">
  <li><%= link_to "Home", root_path %></li>
  <li><%= link_to "Developers", developers_path %></li>
  <li class="active">Education</li>
</ol>
<% end %>

<div class="page-header">
  <h1>Education</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-hover vertical-align-middle" width="100%">
    <thead>
      <tr>
        <th>API</th>
        <th>Version</th>
        <th>Description</th>
        <th>Updated</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
     <%= render partial: 'developers/developers_list_item',
     locals: { name: 'Post 16 Results', target: method(:api_v10_education_post16results_path), desc: 'Post 16 results since 2010 by type, grade and percentage',
     updated: '13/04/2015' } %>
     <%= render partial: 'developers/developers_list_item',
     locals: { name: 'GCSEs Overall', target: method(:api_v10_education_gcse_overall_path), desc: 'GCSE overall since 2009 by result and percentage',
     updated: '13/04/2015' } %>
     <%= render partial: 'developers/developers_list_item',
     locals: { name: 'GCSEs By School', target: method(:api_v10_education_gcse_school_path), desc: 'GCSE results for schools since 2010 by result and percentage',
     updated: '13/04/2015' } %>
    <%= render partial: 'developers/developers_list_item',
     locals: { name: 'Students in the UK', target: method(:api_v10_education_students_in_uk_path), desc: 'Students studying in the UK',
     updated: '16/10/2015' } %>
   </tbody>
 </table>
</div>

<p>Source: <%= link_to 'education.gg', 'http://www.education.gg' %></p>
<p>Source: <%= link_to 'gov.gg', 'http://www.gov.gg/pru' %></p>
